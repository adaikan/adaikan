{
	"name": "adaikan-dashboard",
	"version": "0.1.0",
	"description": "ada ikan web application, for admin",
	"type": "module",
	"engines": {
		"node": "14.18.0"
	},
	"scripts": {
		"start": "HOST=0.0.0.0 PORT=3050 node build/index.js",
		"dev": "svelte-kit dev --port 3050",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview --port 3050",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"tailwind:dev": "npx tailwindcss -w -i ./src/styles/tailwind.css -o ./static/tailwind.css"
	},
	"devDependencies": {
		"@sveltejs/kit": "^1.0.0-next.189",
		"@tailwindcss/forms": "^0.3.4",
		"@tailwindcss/typography": "^0.4.1",
		"@types/cookie": "^0.4.1",
		"autoprefixer": "^10.3.6",
		"cssnano": "^5.0.8",
		"dotenv": "^10.0.0",
		"postcss": "^8.3.11",
		"postcss-load-config": "^3.1.0",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"sass": "^1.43.3",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.9.7",
		"tailwindcss": "^2.2.17",
		"tslib": "^2.3.1",
		"typescript": "^4.4.4",
		"vite-plugin-live-reload": "^2.1.0"
	},
	"dependencies": {
		"@fontsource/fira-mono": "^4.5.0",
		"@lukeed/uuid": "^2.0.0",
		"@mankins/svelte-adapter-express": "0.0.5",
		"@sveltejs/adapter-node": "^1.0.0-next.55",
		"@sveltejs/adapter-static": "^1.0.0-next.21",
		"@tailwindcss/aspect-ratio": "^0.2.1",
		"@themesberg/flowbite": "^1.0.4",
		"cookie": "^0.4.1",
		"daisyui": "^1.16.0"
	}
}
